{
  "name": "replay-chrome-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "build": "./versionsync.sh && INLINE_RUNTIME_CHUNK=false react-scripts build",
    "prod": "npm run build && jq -r '.version' public/manifest.json | xargs -I {} zip -r sauce-labs-replay-{}.zip build"
  },
  "devDependencies": {
    "react-scripts": "5.0.1"
  },
  "browserslist": ["last 2 chrome versions"]
}
